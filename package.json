{
  "homepage": "https://eunsu-resume.vercel.app/",
  "name": "eunsu-resume",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "pnpm lint && pnpm prettier && next build && next-sitemap",
    "db:studio": "drizzle-kit studio",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx ./src/db/configs/migrate.ts",
    "db:push": "pnpm drizzle-kit push",
    "db:pull": "pnpm drizzle-kit pull",
    "start": "next start",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "prettier": "prettier --config .prettierrc --write \"**/*.{ts,tsx,md}\"",
    "serve": "serve out",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d out"
  },
  "dependencies": {
