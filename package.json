    "@radix-ui/react-tooltip": "^1.2.8",
    "@tailwindcss/postcss": "^4.1.10",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.44.7",
    "js-yaml": "^4.1.0",
    "lucide-react": "^0.469.0",
    "motion": "^12.6.2",
    "next": "15.3.3",
    "prettier": "^3.4.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^9.0.3",
